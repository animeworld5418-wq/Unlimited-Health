local TweenService = game:GetService("TweenService")
local Player = game.Players.LocalPlayer
local PlayerGui = Player:WaitForChild("PlayerGui")

-------------------------------------------------
-- VALID KEYS (50 STRONG, SINGLE USE)
-------------------------------------------------
local validKeys = {
    "H3althX#N9m2Qz!", "D3v!lP0wR#T7xA4", "F9sT#B8v!k2nY5", "Q2r!V3pL#x7T8j",
    "R6o!bC1#nT5mP8", "K4e!xF9#qR7yH2", "A3v!tS6#bM8lN5", "W2j!oK7#rE4xT1",
    "U5m!Z8#nP9qF3r", "L8k!hN2#vB6mQ7", "Y9t!R3#cF8pL1x", "Z7n!V5#jD4mE9r",
    "B2a!tX6#rP1nY8", "C9f!L4#bK7yR2m", "G5u!J8#nW3tE1p", "N4x!M2#lS9kD6v",
    "S3z!B7#hT5mF8y", "E1v!H9#rG2qP6n", "O6p!L8#xM3jV4t", "I9n!T5#eR7cK2y",
    "T2b!Y4#sW9mJ3p", "M8q!D1#vP6nL5r", "V7k!G3#tS2jQ9x", "P9f!J6#rH4mB8n",
    "J3l!N7#xF9tR2q", "X1m!E5#vC8nY7p", "Q4r!K2#zW6hD9t", "L6p!T3#cG8nA1v",
    "K9x!V8#fB5mH2y", "A7r!P4#qN1jT9x", "Z8t!L5#bE3nC7v", "B6n!G9#yS2mD1p",
    "U2f!H7#xT9vL5r", "Y5k!Q4#cM8nW3p", "R3v!X2#jF9tB7q", "C1m!Z6#rE8nK4y",
    "E9q!P5#bT7mL2v", "N7t!S3#xD4nY8p", "G4n!H9#rV2mC6q", "V5x!M8#yN1kT7p",
    "O8r!J2#qF9bL4v", "T6m!E3#xC5nY9p", "S9f!L7#rP2kG1v", "F2k!H8#yM4nT3q",
    "D3x!V9#cR6mJ7p", "P1l!N5#tY8qK2v", "J5v!C2#xE4mL9r", "K8m!Q6#nW3bH1p",
    "H9n!S7#vF5pD2q", "Z2t!R8#kL3mY6x"
