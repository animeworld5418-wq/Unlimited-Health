local usedKeys = {}

-------------------------------------------------
-- SCREEN GUI
-------------------------------------------------
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "DevilKeySystem"
screenGui.Parent = PlayerGui
screenGui.ResetOnSpawn = false

-------------------------------------------------
-- KEY INPUT UI
-------------------------------------------------
local keyFrame = Instance.new("Frame")
keyFrame.Size = UDim2.new(0, 420, 0, 220)
keyFrame.Position = UDim2.new(0.5, -210, 0.5, -110)
keyFrame.BackgroundColor3 = Color3.fromRGB(18, 18, 18)
keyFrame.BorderSizePixel = 0
keyFrame.Parent = screenGui
Instance.new("UICorner", keyFrame).CornerRadius = UDim.new(0, 12)

local title = Instance.new("TextLabel")
title.Size = UDim2.new(1, 0, 0, 50)
title.Position = UDim2.new(0, 0, 0, 8)
title.BackgroundTransparency = 1
title.Text = "DEVIL SPEED BOOST SCRIPT"
title.Font = Enum.Font.GothamBold
title.TextScaled = true
title.TextColor3 = Color3.new(1,1,1)
title.Parent = keyFrame

local keyBox = Instance.new("TextBox")
keyBox.Size = UDim2.new(0.86, 0, 0, 48)
keyBox.Position = UDim2.new(0.07, 0, 0.35, 0)
keyBox.PlaceholderText = "Enter your access key..."
keyBox.Text = ""
keyBox.Font = Enum.Font.Gotham
keyBox.TextScaled = true
keyBox.TextColor3 = Color3.new(1,1,1)
keyBox.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
keyBox.Parent = keyFrame
Instance.new("UICorner", keyBox).CornerRadius = UDim.new(0, 10)

local submitButton = Instance.new("TextButton")
submitButton.Size = UDim2.new(0.5, 0, 0, 44)
submitButton.Position = UDim2.new(0.25, 0, 0.74, 0)
submitButton.Text = "SUBMIT"
submitButton.Font = Enum.Font.GothamBold
submitButton.TextScaled = true
submitButton.TextColor3 = Color3.new(1,1,1)
submitButton.BackgroundColor3 = Color3.fromRGB(0, 210, 120)
submitButton.Parent = keyFrame
Instance.new("UICorner", submitButton).CornerRadius = UDim.new(0, 10)

-------------------------------------------------
-- POPUP NOTIFICATION
-------------------------------------------------
local function showPopup(text, color)
	local popup = Instance.new("Frame")
	popup.Size = UDim2.new(0, 360, 0, 60)
	popup.Position = UDim2.new(0.5, -180, 0.72, 0)
	popup.BackgroundColor3 = Color3.fromRGB(10, 10, 10)
	popup.BorderSizePixel = 0
	popup.Parent = screenGui
	Instance.new("UICorner", popup).CornerRadius = UDim.new(0, 10)

	local lbl = Instance.new("TextLabel")
	lbl.Size = UDim2.new(1, -20, 1, -10)
	lbl.Position = UDim2.new(0, 10, 0, 5)
	lbl.BackgroundTransparency = 1
	lbl.Text = text
	lbl.TextColor3 = color or Color3.new(1,1,1)
	lbl.Font = Enum.Font.GothamBold
	lbl.TextScaled = true
	lbl.Parent = popup

	TweenService:Create(popup, TweenInfo.new(0.35), {BackgroundTransparency = 0.0}):Play()
	task.wait(1.4)
	TweenService:Create(popup, TweenInfo.new(0.45), {BackgroundTransparency = 1, Size = UDim2.new(0, 0, 0, 0)}):Play()
	task.wait(0.45)
	popup:Destroy()
end

-------------------------------------------------
-- KEY VALIDATION
-------------------------------------------------
submitButton.MouseButton1Click:Connect(function()
	local key = tostring(keyBox.Text or ""):gsub("^%s*(.-)%s*$", "%1")
	if key == "" then
		showPopup("Enter a key first", Color3.fromRGB(255,200,50))
		return
	end

	if usedKeys[key] then
		showPopup("❌ Key already used", Color3.fromRGB(255,80,80))
		keyBox.Text = ""
		return
	end

	if table.find(validKeys, key) then
		usedKeys[key] = true
		showPopup("✅ Key accepted, starting script...", Color3.fromRGB(120,255,140))
		task.wait(0.8)
		keyFrame:Destroy()

		-- LOAD MAIN SCRIPT
		local url = "https://raw.githubusercontent.com/animeworld5418-wq/Unlimited-Health/refs/heads/main/Speed%20Boost"
		loadstring(game:HttpGet(url))()
	else
		showPopup("❌ Invalid key", Color3.fromRGB(255,80,80))
		keyBox.Text = ""
	end
end)
